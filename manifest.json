{
    "extension": 
    {
        "name": "wakanda-extension-mobile-test",
        "version": "1.0.0",
        "description": "tests",
        "icon": "mobilePreview.png",
        "senders": [
            {
                "location": "studioToolbar", 
		    	"actionName": "launchBuild",
                "icon": "mobileBuild.png",
                "alternateIcon": "mobileBuild_alt.png",
                "title": "Build",
                "tips": "Build Mobile App",
                "menu": [
                    { "actionName": "androidBuild" },
                    { "actionName": "iosBuild" }
                ]
            },
            {
                "location": "studioToolbar", 
		    	"actionName": "launchRun",
                "icon": "mobileRun.png",
                "alternateIcon": "mobileRun_alt.png",
                "title": "Run",
                "tips": "Mobile run",
                "id": "configRun",
                "menu": [
                    { "actionName": "androidEmulate" },
                    { "actionName": "iosEmulate" },
                    { "separator": {} },
                    { "actionName": "androidRun" },
                    { "actionName": "iosRun" }
                ]
            },
            {
                "location": "studioToolbar", 
		    	"actionName": "launchTest",
                "icon": "mobilePreview.png",
                "alternateIcon": "mobilePreview.png",
                "title": "Preview",
                "tips": "Preview Mobile App",
                "menu": [
                    { "actionName": "studioPreview" },
                    { "actionName": "browserPreview" },
                    { "separator": {} },
                    { "actionName": "androidTest" },
                    { "actionName": "iosTest" }
                ]
            }
        ],
        "actions": [
            {
                "name": "studioStartHandler",
                "title": "studio start handler",
                "trigger": [
                    { "event": "onStudioStart" }
                ]
            },
            {
                "name": "solutionOpenedHandler",
                "title": "solution opened handler",
                "trigger": [
                    { "event": "onSolutionOpened" }
                ]
            },
            {
                "name": "solutionClosedHandler",
                "title": "solution closed handler",
                "trigger": [
                    { "event": "onSolutionClosed" }
                ]
            },
            {
                "name": "solutionBeforeClosingHandler",
                "title": "solution closed handler",
                "trigger": [
                    { "event": "onSolutionBeforeClosing" }
                ]
            },
            {
                "name": "launchTest",
                "title": "Test"
            },
            {
                "name": "studioPreview",
                "title": "Preview in New Tab"
            },
            {
                "name": "browserPreview",
                "title": "Preview in Web Browser"
            },
            {
                "name": "androidTest",
                "title": "Preview Android App"
            },
            {
                "name": "iosTest",
                "title": "Preview iOS App"
            },
            {
                "name": "launchRun",
                "title": "Run"
            },
            {
                "name": "androidRun",
                "title": "Run on Android Devices"
            },
            {
                "name": "iosRun",
                "title": "Run on iOS Devices"
            },
            {
                "name": "androidEmulate",
                "title": "Run on Android Emulator"
            },
            {
                "name": "iosEmulate",
                "title": "Run on iOS Simulator"
            },
            {
                "name": "launchBuild",
                "title": "Build & deploy"
            },
            {
                "name": "androidBuild",
                "title": "Build Android App"
            },
            {
                "name": "iosBuild",
                "title": "Build iOS App"
            },
            {
                "name": "menuOpened",
                "title": "Test Menu Opening",
                "trigger": [
                    { "event": "onToolbarButtonMenuOpen" }
                ]
            },
            {
                "name": "launchWebPreview",
                "title": "Web Run"
            },
            {
                "name": "webStudioPreview",
                "title": "Run in New Tab"
            },
            {
                "name": "webBrowserPreview",
                "title": "Run in Web Browser"
            }
        ],
        "lifetime": "application_lifetime"
    }
